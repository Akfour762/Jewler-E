CREATE TABLE supplier (
	supplier_id INT NOT NULL AUTO_INCREMENT PRIMARY KEY,
	companyname VARCHAR(50) NOT NULL,
	pointofcontact VARCHAR(50) NOT NULL,
	website VARCHAR(50) NOT NULL,
	phone VARCHAR(20) NOT NULL
	);


